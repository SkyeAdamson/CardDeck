<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <!-- NuGet Metadata -->
        <PackageId>SimpleCardDeck</PackageId>
        <Version>1.0.0</Version>
        <Authors>Skye</Authors>
        <Company>Skye</Company>
        <Description>A simple library for a simple card deck.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/SkyeAdamson/CardDeck</PackageProjectUrl>
        <RepositoryUrl>https://github.com/SkyeAdamson/CardDeck</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

</Project>
